{
  "name": "honey-store",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start:frontend": "nx serve frontend",
    "start:backend": "nx serve backend",
    "start:payment": "nx serve payment-service",
    "build": "nx affected --target=build --parallel=3 --base=master",
    "build:all": "nx run-many --target=build --all --parallel=3",
    "build:status": "nx show projects --affected --type=app",
    "test": "nx affected --target=test --parallel=3 --base=master",
    "test:all": "nx run-many --target=test --all --parallel=3",
    "build-and-deploy": "./scripts/build-and-deploy.sh",
    "port-forward": "./scripts/port-forward.sh",
    "ngrok:start": "./scripts/ngrok-start.sh",
    "telepresence:start": "./scripts/telepresence-start.sh",
    "version:frontend": "cd apps/frontend && pnpm version:bump",
    "version:backend": "cd apps/backend && pnpm version:bump",
    "version:payment": "cd apps/payment-service && pnpm version:bump",
    "version:all": "pnpm version:frontend && pnpm version:backend && pnpm version:payment",
    "watch": "./scripts/watch.sh",
    "k8s:build:backend": "./scripts/k8s-build-service.sh backend",
    "k8s:build:frontend": "./scripts/k8s-build-service.sh frontend",
    "k8s:build:payment": "./scripts/k8s-build-service.sh payment-service",
    "k8s:build:all": "./scripts/k8s-build.sh",
    "k8s:deploy:backend": "./scripts/k8s-deploy-service.sh backend ${K8S_NAMESPACE:-meetup3}",
    "k8s:deploy:frontend": "./scripts/k8s-deploy-service.sh frontend ${K8S_NAMESPACE:-meetup3}",
    "k8s:deploy:payment": "./scripts/k8s-deploy-service.sh payment-service ${K8S_NAMESPACE:-meetup3}",
    "k8s:deploy:all": "pnpm k8s:deploy:backend && pnpm k8s:deploy:frontend",
    "k8s:build-deploy:backend": "./scripts/k8s-deploy-service.sh backend ${K8S_NAMESPACE:-meetup3}",
    "k8s:build-deploy:frontend": "./scripts/k8s-deploy-service.sh frontend ${K8S_NAMESPACE:-meetup3}",
    "k8s:build-deploy:payment": "./scripts/k8s-deploy-service.sh payment-service ${K8S_NAMESPACE:-meetup3}",
    "deploy": "./scripts/deploy.sh",
    "k8s:status": "kubectl get pods -n ${K8S_NAMESPACE:-meetup3}",
    "k8s:logs:backend": "kubectl logs -n ${K8S_NAMESPACE:-meetup3} -l app=backend --tail=50 -f",
    "k8s:logs:frontend": "kubectl logs -n ${K8S_NAMESPACE:-meetup3} -l app=frontend --tail=50 -f"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "express": "^4.18.2",
    "mongoose": "^8.0.0",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-devkit/core": "^17.0.0",
    "@angular-devkit/schematics": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@nx/angular": "^18.0.0",
    "@nx/express": "^18.0.0",
    "@nx/node": "^18.0.0",
    "@nx/webpack": "^18.0.0",
    "@nx/workspace": "^18.0.0",
    "@nx/js": "^18.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "nx": "^18.0.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.2"
  }
}
